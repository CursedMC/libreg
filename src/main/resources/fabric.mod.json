{
  "schemaVersion": 1,
  "id": "libreg",
  "version": "${version}",

  "name": "libreg",
  "description": "A tiny Kotlin library for registering blocks, items, biomes, dimensions, etc.",
  "authors": [
    "Tehc"
  ],
  "contact": {
    "sources": "https://github.com/CursedMC/libreg"
  },

  "license": "Apache-2.0",
  "icon": "assets/libreg/icon.png",

  "entrypoints": {
    "main": [
      "dev.tehc.libreg.Mod::main"
    ],
    "client": [
      "dev.tehc.libreg.client.ModClient::main"
    ],
    "server": [
    ]
  },
  "mixins": [
    "libreg.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.11",
    "fabric": ">=0.36",
    "fabric-language-kotlin": "*",
    "minecraft": "1.17.x"
